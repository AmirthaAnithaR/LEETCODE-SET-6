bool threeConsecutiveOdds(int* arr, int arrSize) {
    int c=0;
     for (int i = 0; i < arrSize; i++) {
    if (arr[i] % 2 == 1) { 
            c++;
            if (c == 3) { 
                return true;
            }
        } else {
            c = 0; 
        }
    }
    
    return false; 
    }
